# Production Environment Variables for Deployment
# Copy this file to .env.production and fill in your actual values

# AWS Region (Asia Pacific Southeast 1)
VITE_AWS_REGION=ap-southeast-1

# AWS Cognito Configuration
# Get these values from your Terraform outputs after deployment
VITE_USER_POOL_ID=ap-southeast-1_XXXXXXXXX
VITE_USER_POOL_CLIENT_ID=XXXXXXXXXXXXXXXXXXXXXXXXXX
VITE_IDENTITY_POOL_ID=ap-southeast-1:xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx

# API Gateway URL
# Get this from your Terraform outputs after deployment
VITE_API_GATEWAY_URL=https://xxxxxxxxxx.execute-api.ap-southeast-1.amazonaws.com/prod

# Application Configuration
VITE_APP_NAME=Law Firm Client Database
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=production
